from PyPDF2 import PdfReader, PdfWriter

filename = r"C:\Users\anamc\OneDrive\Documents\Health insurance\Claim form half empty.pdf" 
output_path = r"C:\Users\anamc\OneDrive\Documents\Health insurance\Auto_claimform4.pdf"

reader = PdfReader(filename)
writer = PdfWriter()

for i in range(0,8):
    page = reader.pages[i]
    writer.add_page(page)

imp_page = reader.pages[2]
fields = reader.get_fields()

print(fields)

dic = {
    "Treatment date": "20302021",
    'Total charge (with currency)': "U genius",
    'Location of claim – Provider’s name and address': "Hellooooooooooo",
    'Location of claim – Provider’s name and address_1': "Hi2",
    'Reason for visit': "Ouchie"
}

for field, text in dic.items():
    writer.update_page_form_field_values(
        writer.pages[2], {field: text}
    )

# also do date signed

# write "output" to PyPDF2-output.pdf
with open(output_path, "wb") as output_stream:
    writer.write(output_stream)